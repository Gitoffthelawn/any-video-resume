{

    "manifest_version": 2,
    "name": "Video Resumer",
    "version": "1.0",
    "description": "Return to last time you stopped the video.",
    "icons": {
      "48": "icons/logo-48.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["./js/content.js"],
        "run_at": "document_end"
      }
    ],
    "permissions": [
        "storage"
    ]
    
  }